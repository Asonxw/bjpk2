[Script]
yl = 300
nowyl = 300
//1挂点
cr1x = 1526
cr1y = 518
//2挂点
cr2x = 1526
cr2y = 548
//3挂点
cr3x = 1526
cr3y = 573
//4挂点
cr4x = 1526
cr4y = 597
//模拟切换点
crflagx = 732
crflagy = 389
//止盈输入点
zyx = 795
zyy = 318
//开始或停止挂机按钮点
btnx = 1493
btny = 375
//开始停止按钮左上角点和右下角点
btnzs_x = 1424
btnzs_y = 355
btnyx_x = 1556
btnyx_y = 397
//初始化止盈值
MoveTo zyx, zyy
LeftClick 1
KeyPress "BackSpace", 5
SayString nowyl
Rem A
//获取四个点的颜色
cr1 = GetPixelColor(cr1x,cr1y)
cr2 = GetPixelColor(cr2x,cr2y)
cr3 = GetPixelColor(cr3x,cr3y)
cr4 = GetPixelColor(cr4x,cr4y)
//获取模拟挂机颜色
crflag = GetPixelColor(crflagx,crflagy)

//MessageBox cr1
If cr1 = "FAF4E3" and cr2 = "FAF4E3" and cr3 = "FAF4E3" and cr4 = "FAF4E3" and crflag = "FFFFFF" Then 
	//如果四个点出现了挂的颜色且为模拟投注时则点击模拟切换为真实
	MoveTo crflagx,crflagy
	LeftClick 1
End If
FindPic 1424,355,1556,397,"E:\gitSource\bjpk2\sc\开始挂机.bmp",0.9,intX,intY
If intX > 0 And intY > 0 Then 
MessageBox crflag
End If

//获取坐标
FindPic btnzs_x,btnzs_y,btnyx_x,btnyx_y,"E:\gitSource\bjpk2\sc\开始挂机.bmp",0.9,intX,intY
 //正式投注模式下，如果按钮为“开始挂机”则证明 已达到止盈限额，需要切换为模拟投注并将止盈限额+400
If crflag = "CCCCCC" and intX > 0 and intY > 0 Then 
	//修改止盈限额+400
	MoveTo zyx, zyy
	LeftClick 1
	KeyPress "BackSpace", 6
	nowyl = nowyl + yl
	SayString nowyl
	//切换投注模式为模拟投注
	MoveTo crflagx,crflagy
	LeftClick 1
	//点击开始挂机按钮
	MoveTo btnx,btny
	LeftClick 1
End if
Delay 3000
goto A
